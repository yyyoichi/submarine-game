web/**/.rts2_cache_umd

# Optional REPL history
web/**/.node_repl_history

# Output of 'npm pack'
web/**/*.tgz

# Yarn Integrity file
web/**/.yarn-integrity

# dotenv environment variable files
web/**/.env
web/**/.env.development.local
web/**/.env.test.local
web/**/.env.production.local
web/**/.env.local

